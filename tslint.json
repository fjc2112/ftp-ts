{
    "defaultSeverity": "warn",
    "extends": [
        "tslint:latest"
    ],
    "jsRules": {},
    "rules": {
        "no-implicit-dependencies": false,
        "no-namespace":false,
        "no-console":false,
        "max-classes-per-file":false,
        "no-bitwise": false,
        "variable-name": [true, "ban-keywords", "check-format", "allow-leading-underscore"],
        "max-line-length": false
    },
    "rulesDirectory": []
}
